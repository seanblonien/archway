[
  {
    "name": "capstones",
    "description": "Describes a Capstone Project",
    "connection": "default",
    "collectionName": "capstones",
    "attributes": {
      "capstoneName": {
        "type": "string",
        "required": true
      },
      "startDate": {
        "type": "date"
      },
      "endDate": {
        "type": "date"
      },
      "description": {
        "type": "text"
      },
      "moderator": {
        "target": "user"
      },
      "displayPhoto": {
        "type": "media",
        "multiple": false
      },
      "pictures": {
        "type": "media",
        "multiple": true
      },
      "department": {
        "nature": "manyToMany",
        "targetAttribute": "capstones",
        "target": "application::sponsors.sponsors",
        "unique": false,
        "dominant": true
      },
      "sponsors": {
        "nature": "manyToMany",
        "targetAttribute": "capstones",
        "target": "application::sponsors.sponsors",
        "unique": false,
        "dominant": true
      },
      "posts": {
        "nature": "manyWay",
        "target": "application::posts.posts",
        "unique": false
      },
      "creators": {
        "nature": "manyToMany",
        "targetAttribute": "capstones",
        "target": "application::users.users",
        "unique": false,
        "dominant": true
      }
    }
  },
  {
    "name": "commemts",
    "description": "Describes a Comment to a Post",
    "attributes": {
      "conetent": {
        "type": "text"
      },
      "username": {
        "type": "string"
      },
      "editable": {
        "type": "bool"
      },
      "post": {
        "model": "posts"
      },
      "user": {
        "model": "user"
      }
    }
  },
  {
    "name": "department",
    "description": "Describes a Department",
    "attributes": {
      "name": {
        "type": "string"
      },
      "description": {
        "type": "text"
      },
      "url": {
        "type": "string"
      },
      "contact": {
        "type": "email"
      }
    }
  },
  {
    "name": "faqs",
    "description": "Describes a FAQ",
    "attributes": {
      "Category": {
        "type": "string"
      },
      "Question": {
        "type": "text"
      },
      "Answer": {
        "type": "text"
      },
      "Url": {
        "type": "string"
      }
    }
  },
  {
    "name": "posts",
    "description": "Describes a Post on a Capstone Project",
    "attributes": {
      "title": {
        "type": "string"
      },
      "content": {
        "type": "text"
      },
      "media": {
        "type": "media",
        "multiple": true
      },
      "user": {
        "model": "user"
      },
      "capstone": {
        "model": "capstones",
        "via": "posts"
      }
    }
  },
  {
    "name": "sponsors",
    "description": "Describes a Sponsor for a Capstone Project",
    "attributes": {
      "name": {
        "type": "string"
      },
      "description": {
        "type": "text"
      },
      "webUrl": {
        "type": "string"
      },
      "logo": {
        "type": "media",
        "multiple": false
      }
    }
  },
  {
    "name": "users-permissions_user",
    "description": "Describes a User",
    "attributes": {
      "Fullname": {
        "type": "string"
      },
      "Department": {
        "type": "string"
      },
      "Bio": {
        "type": "text"
      },
      "Description": {
        "type": "richtext"
      },
      "ProfilePicture": {
        "type": "media",
        "multiple": false
      },
      "moderator": {
        "model": "user"
      },
      "createdCapstones": {
        "collection": "capstones",
        "via": "user"
      }
    }
  },
  {
    "name": "viewstats",
    "description": "Describes a ViewStat",
    "attributes": {
      "count": {
        "type": "integer"
      },
      "user": {
        "model": "user"
      },
      "department": {
        "model": "departments"
      }
    }
  }
]
