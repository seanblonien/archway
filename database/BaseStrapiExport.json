{
  "application": [
    {
      "name": "capstones",
      "description": "Describes a Capstone Project",
      "connection": "default",
      "kind": "collectionType",
      "collectionName": "capstones",
      "attributes": {
        "name": {
          "type": "string",
          "required": true
        },
        "isFeatured": {
          "type": "boolean",
          "default": false
        },
        "startDate": {
          "type": "date",
          "required": true
        },
        "endDate": {
          "type": "date",
          "required": true
        },
        "description": {
          "type": "richtext",
          "required": true,
          "maxLength": 10000
        },
        "preview": {
          "type": "string"
        },
        "coverPhoto": {
          "type": "media",
          "multiple": false,
          "required": false
        },
        "media": {
          "type": "media",
          "multiple": true,
          "required": false
        },
        "sponsors": {
          "nature": "manyToMany",
          "target": "application::sponsors.sponsors",
          "dominant": true,
          "targetAttribute": "capstones",
          "unique": false
        },
        "members": {
          "nature": "manyToMany",
          "target": "plugins::users-permissions.user",
          "dominant": true,
          "targetAttribute": "capstones",
          "unique": false
        },
        "department": {
          "nature": "oneToMany",
          "target": "application::departments.departments",
          "dominant": false,
          "targetAttribute": "capstones",
          "unique": false
        },
        "section": {
          "nature": "manyToOne",
          "target": "application::sections.sections",
          "dominant": false,
          "targetAttribute": "capstones",
          "unique": false
        },
        "thumbnail": {
          "type": "media",
          "multiple": false,
          "required": false
        }
      }
    },
    {
      "name": "courses",
      "description": "",
      "connection": "default",
      "kind": "collectionType",
      "collectionName": "courses",
      "attributes": {
        "title": {
          "type": "string",
          "required": true
        },
        "subjectAndNumber": {
          "type": "string",
          "required": true
        },
        "creditHours": {
          "type": "decimal"
        },
        "description": {
          "type": "richtext",
          "maxLength": 10000
        },
        "url": {
          "type": "string"
        },
        "sections": {
          "nature": "oneToMany",
          "target": "application::sections.sections",
          "dominant": false,
          "targetAttribute": "course",
          "unique": false
        },
        "department": {
          "nature": "manyToOne",
          "target": "application::departments.departments",
          "dominant": false,
          "targetAttribute": "courses",
          "unique": false
        }
      }
    },
    {
      "name": "departments",
      "description": "Describes a Department",
      "connection": "default",
      "kind": "collectionType",
      "collectionName": "departments",
      "attributes": {
        "name": {
          "type": "string",
          "unique": true,
          "required": true
        },
        "capstones": {
          "nature": "manyToOne",
          "target": "application::capstones.capstones",
          "dominant": false,
          "targetAttribute": "department",
          "unique": false
        },
        "url": {
          "type": "string"
        },
        "contact": {
          "type": "email"
        },
        "description": {
          "type": "richtext",
          "default": "10000"
        },
        "preview": {
          "type": "string"
        },
        "thumbnail": {
          "type": "media",
          "multiple": false,
          "required": false
        },
        "coverPhoto": {
          "type": "media",
          "multiple": false,
          "required": false
        },
        "proposals": {
          "nature": "manyToMany",
          "target": "application::proposals.proposals",
          "dominant": true,
          "targetAttribute": "departments",
          "unique": false
        },
        "professors": {
          "nature": "oneToMany",
          "target": "plugins::users-permissions.user",
          "dominant": false,
          "targetAttribute": "department",
          "unique": false
        },
        "courses": {
          "nature": "oneToMany",
          "target": "application::courses.courses",
          "dominant": false,
          "targetAttribute": "department",
          "unique": false
        }
      }
    },
    {
      "name": "faqs",
      "description": "Describes a FAQ",
      "connection": "default",
      "kind": "collectionType",
      "collectionName": "faqs",
      "attributes": {
        "category": {
          "type": "string"
        },
        "question": {
          "type": "string",
          "required": true
        },
        "answer": {
          "type": "richtext",
          "required": true,
          "maxLength": 10000
        }
      }
    },
    {
      "name": "home page",
      "description": "",
      "connection": "default",
      "kind": "singleType",
      "collectionName": "home_pages",
      "attributes": {
        "infoparagraph": {
          "type": "richtext"
        },
        "bgimage": {
          "type": "media",
          "multiple": false,
          "required": false
        }
      }
    },
    {
      "name": "proposals",
      "description": "Describes a proposal made by a sponsor",
      "connection": "default",
      "kind": "collectionType",
      "collectionName": "posts",
      "attributes": {
        "projectTitle": {
          "type": "string",
          "unique": false,
          "default": "New Proposal"
        },
        "phone": {
          "type": "string",
          "unique": false
        },
        "isIntellectualPropertyRequired": {
          "type": "boolean"
        },
        "isNondisclosureRequired": {
          "type": "boolean"
        },
        "dateSubmitted": {
          "type": "date",
          "unique": false
        },
        "creator": {
          "nature": "oneWay",
          "target": "plugins::users-permissions.user",
          "dominant": false,
          "unique": false
        },
        "departments": {
          "nature": "manyToMany",
          "target": "application::departments.departments",
          "dominant": false,
          "targetAttribute": "proposals",
          "unique": false
        },
        "status": {
          "type": "enumeration",
          "enum": [
            "notSubmitted",
            "submittedUnapproved",
            "submittedApproved"
          ],
          "default": "notSubmitted"
        },
        "dateApproved": {
          "type": "date"
        },
        "projectUse": {
          "type": "richtext"
        },
        "financialSupport": {
          "type": "richtext"
        },
        "projectDeliverables": {
          "type": "richtext"
        },
        "projectDescription": {
          "type": "richtext"
        },
        "email": {
          "type": "email"
        },
        "sponsors": {
          "nature": "manyToMany",
          "target": "application::sponsors.sponsors",
          "dominant": true,
          "targetAttribute": "proposals",
          "unique": false
        }
      }
    },
    {
      "name": "sections",
      "description": "",
      "connection": "default",
      "kind": "collectionType",
      "collectionName": "sections",
      "attributes": {
        "capstones": {
          "nature": "oneToMany",
          "target": "application::capstones.capstones",
          "dominant": false,
          "targetAttribute": "section",
          "unique": false
        },
        "instructors": {
          "nature": "manyToMany",
          "target": "plugins::users-permissions.user",
          "dominant": true,
          "targetAttribute": "instructorSections",
          "unique": false
        },
        "course": {
          "nature": "manyToOne",
          "target": "application::courses.courses",
          "dominant": false,
          "targetAttribute": "sections",
          "unique": false
        },
        "students": {
          "nature": "manyToMany",
          "target": "plugins::users-permissions.user",
          "dominant": true,
          "targetAttribute": "studentSections",
          "unique": false
        },
        "Semester": {
          "type": "enumeration",
          "enum": [
            "fall",
            "spring"
          ],
          "required": true
        },
        "year": {
          "type": "integer",
          "required": true
        }
      }
    },
    {
      "name": "sponsor page",
      "description": "",
      "connection": "default",
      "kind": "singleType",
      "collectionName": "sponsor_pages",
      "attributes": {
        "leftcolumn": {
          "type": "richtext"
        },
        "rightcolumn": {
          "type": "richtext"
        },
        "bgimage": {
          "type": "media",
          "multiple": false,
          "required": false
        },
        "main_paragraph": {
          "type": "richtext"
        }
      }
    },
    {
      "name": "sponsors",
      "description": "Describes a Sponsor for a Capstone Project",
      "connection": "default",
      "kind": "collectionType",
      "collectionName": "sponsors",
      "attributes": {
        "name": {
          "type": "string",
          "required": true
        },
        "capstones": {
          "nature": "manyToMany",
          "target": "application::capstones.capstones",
          "dominant": false,
          "targetAttribute": "sponsors",
          "unique": false
        },
        "url": {
          "type": "string"
        },
        "logo": {
          "type": "media",
          "multiple": false,
          "required": false
        },
        "thumbnail": {
          "type": "media",
          "multiple": false,
          "required": false
        },
        "description": {
          "type": "richtext",
          "required": false,
          "maxLength": 10000
        },
        "preview": {
          "type": "string"
        },
        "isFeatured": {
          "type": "boolean"
        },
        "coverPhoto": {
          "type": "media",
          "multiple": false,
          "required": false
        },
        "proposals": {
          "nature": "manyToMany",
          "target": "application::proposals.proposals",
          "dominant": false,
          "targetAttribute": "sponsors",
          "unique": false
        },
        "personnel": {
          "nature": "oneToMany",
          "target": "plugins::users-permissions.user",
          "dominant": false,
          "targetAttribute": "sponsorOrganization",
          "unique": false
        }
      }
    },
    {
      "name": "theme",
      "description": "",
      "connection": "default",
      "kind": "singleType",
      "collectionName": "themes",
      "attributes": {
        "primaryColor": {
          "type": "string"
        },
        "secondaryColor": {
          "type": "string"
        },
        "backgroundColor": {
          "type": "string"
        },
        "errorColor": {
          "type": "string"
        },
        "activeColor": {
          "type": "string"
        },
        "hoverColor": {
          "type": "string"
        },
        "selectedColor": {
          "type": "string"
        },
        "universityName": {
          "type": "string"
        }
      }
    }
  ],
  "plugins": [
    {
      "name": "permission",
      "description": "",
      "connection": "default",
      "kind": "collectionType",
      "collectionName": "users-permissions_permission",
      "attributes": {
        "type": {
          "type": "string",
          "required": true,
          "configurable": false
        },
        "controller": {
          "type": "string",
          "required": true,
          "configurable": false
        },
        "action": {
          "type": "string",
          "required": true,
          "configurable": false
        },
        "enabled": {
          "type": "boolean",
          "required": true,
          "configurable": false
        },
        "policy": {
          "type": "string",
          "configurable": false
        },
        "role": {
          "nature": "manyToOne",
          "target": "plugins::users-permissions.role",
          "dominant": false,
          "targetAttribute": "permissions",
          "configurable": false,
          "unique": false
        }
      }
    },
    {
      "name": "role",
      "description": "",
      "connection": "default",
      "kind": "collectionType",
      "collectionName": "users-permissions_role",
      "attributes": {
        "name": {
          "type": "string",
          "minLength": 3,
          "required": true,
          "configurable": false
        },
        "description": {
          "type": "string",
          "configurable": false
        },
        "type": {
          "type": "string",
          "unique": true,
          "configurable": false
        },
        "permissions": {
          "nature": "oneToMany",
          "target": "plugins::users-permissions.permission",
          "dominant": false,
          "targetAttribute": "role",
          "configurable": false,
          "unique": false
        },
        "users": {
          "nature": "oneToMany",
          "target": "plugins::users-permissions.user",
          "dominant": false,
          "targetAttribute": "role",
          "configurable": false,
          "unique": false
        }
      }
    },
    {
      "name": "user",
      "description": "",
      "connection": "default",
      "kind": "collectionType",
      "collectionName": "users-permissions_user",
      "attributes": {
        "username": {
          "type": "string",
          "minLength": 3,
          "unique": true,
          "configurable": false,
          "required": true
        },
        "email": {
          "type": "email",
          "minLength": 6,
          "configurable": false,
          "required": true
        },
        "provider": {
          "type": "string",
          "configurable": false
        },
        "password": {
          "type": "password",
          "minLength": 6,
          "configurable": false,
          "private": true
        },
        "resetPasswordToken": {
          "type": "string",
          "configurable": false,
          "private": true
        },
        "confirmed": {
          "type": "boolean",
          "default": false,
          "configurable": false
        },
        "blocked": {
          "type": "boolean",
          "default": false,
          "configurable": false
        },
        "role": {
          "nature": "manyToOne",
          "target": "plugins::users-permissions.role",
          "dominant": false,
          "targetAttribute": "users",
          "configurable": false,
          "unique": false
        },
        "Fullname": {
          "type": "string",
          "minLength": 3,
          "unique": true,
          "configurable": false,
          "required": true
        },
        "description": {
          "type": "richtext",
          "maxLength": 10000
        },
        "picture": {
          "type": "media",
          "multiple": false,
          "required": false
        },
        "capstones": {
          "nature": "manyToMany",
          "target": "application::capstones.capstones",
          "dominant": false,
          "targetAttribute": "members",
          "unique": false
        },
        "department": {
          "nature": "manyToOne",
          "target": "application::departments.departments",
          "dominant": false,
          "targetAttribute": "professors",
          "unique": false
        },
        "instructorSections": {
          "nature": "manyToMany",
          "target": "application::sections.sections",
          "dominant": false,
          "targetAttribute": "instructors",
          "unique": false
        },
        "studentSections": {
          "nature": "manyToMany",
          "target": "application::sections.sections",
          "dominant": false,
          "targetAttribute": "students",
          "unique": false
        },
        "sponsorOrganization": {
          "nature": "manyToOne",
          "target": "application::sponsors.sponsors",
          "dominant": false,
          "targetAttribute": "personnel",
          "unique": false
        },
        "jobTitle": {
          "type": "string"
        }
      }
    }
  ]
}